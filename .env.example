# Facebook credentials (use manual_login.py script instead)
FB_EMAIL=your_email@example.com
FB_PASSWORD=your_password
FB_GROUP_ID=your_group_id

# OpenRouter API (for LLM-based decisions, optional)
OPENROUTER_API_KEY=sk-or-your-key-here
OPENROUTER_MODEL=google/gemini-2.5-pro-preview-06-05

# Telegram Bot (create via @BotFather)
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_ADMIN_ID=your_telegram_user_id

# Browser settings
HEADLESS=true
SLOW_MO=100

# Polling settings with stealth jitter
POLL_INTERVAL=3600
POLL_JITTER=0.3

# Proxy rotation (optional, for stealth - comma-separated list)
# PROXY_LIST=["http://user:pass@proxy1:8080","socks5://proxy2:1080"]

# Debug overlay for click validation
DEBUG_CLICK_OVERLAY=true

